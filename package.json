{
  "name": "reactjs-basics",
  "version": "1.0.0",
  "description": "Some basic ReactJs",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && xcopy \"src/index.html\" \"dist/\" /F /Y && webpack-dev-server --content-base src/ --inline --hot",
